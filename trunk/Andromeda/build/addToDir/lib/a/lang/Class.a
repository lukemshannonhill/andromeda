package a.lang => Class;

final class Class{
	private int id;
	private string name;
	
	/**
	*	Classes are only instanced by Andromeda itself
	*/
	private Class(int id, string name){
		this.id = id;
		this.name = name;
	}
	
	string getName(){
		return name;
	}	

	int getId(){
		return id;
	}		
}